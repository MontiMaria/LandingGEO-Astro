        <ContenidoAnimado class="h-full">
          <CardSpotlight title="+500" text="Docentes conectados" titleSize="text-4xl md:text-5xl" layout="default" />
        </ContenidoAnimado>
        <ContenidoAnimado class="h-full">
          <CardSpotlight title="+4500" text="Familias vinculadas" titleSize="text-4xl md:text-5xl" layout="default" />
        </ContenidoAnimado>
        <ContenidoAnimado class="h-full">
          <CardSpotlight title="+3500" text="Estudiantes alcanzados" titleSize="text-4xl md:text-5xl" layout="default" />
